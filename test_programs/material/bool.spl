xor(a, b) :: Bool Bool -> Bool {
	return (a || b) && !(a && b);
}

implies(a, b) :: Bool Bool -> Bool {
	if (!a) {return True;}
	else {return b;}
}

main() {
    println( xor(False, True) );
    println( xor(False, False) );
    println( implies(False, False) );
    println( implies(True, False) );
}